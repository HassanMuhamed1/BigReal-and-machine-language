// Problem-3 =>Hassan
#include <bits/stdc++.h>
#include <string>
#define Hassan ios_base::sync_with_stdio(false); cin.tie(nullptr);
using namespace std;

void split(string target, string delimiter){
    string res;
    for (int i = 0; i < target.size(); ++i) {
        if(target[i]==',' || target[i]==' ')res+=delimiter;
        else if(i==0 || i==target.size())res+='"';
        else{
            res+=target[i];
        }

    }
    cout<<res;
}
int main() {
    Hassan;
    string target , delimiter;
    getline(cin ,target);
    getline(cin ,delimiter);
    split(target, delimiter);
    return 0;
}
